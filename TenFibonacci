public class ten_fibonacci_numbers {

	public static void main(String[] args) {
		
		
		int num1 = 1;
		int num2 = 1;
		int num3;
		int a = 0;
		System.out.println(num1);
		System.out.println(num2);
		
		while (a<9) {
			
		num3 = num1 + num2;
		num1 = num2;
		num2 = num3;
		System.out.println(num3);
		a = a + 1;
		
		}
		
		
	}

}
